{
  "name": "flexicon",
  "version": "0.1.3",
  "main": "./dist/flexicon.json",
  "homepage": "https://github.com/blivesta/flexicon",
  "description": "A Preview and Source Code Copy for SVG Icons.",
  "repository": {
    "type": "git",
    "url": "git@github.com:blivesta/flexicon.git"
  },
  "author": {
    "name": "blivesta",
    "url": "http://www.blivesta.com/"
  },
  "license": "MIT",
  "scripts": {
    "css:cpoy": "cp -r ./node_modules/sircus/dist/sircus.min.css ./dist/sircus.min.css ",
    "serve": "http-server ./dist -o",
    "build": "npm run css:cpoy && node ./flexicon.js",
    "start": "npm run build && npm run serve",
    "deploy": "git subtree push --prefix dist origin gh-pages"
  },
  "devDependencies": {
    "flexicon-generator": "^0.0.3",
    "http-server": "^0.8.5",
    "sircus": "^2.0.0"
  }
}
